{
  "name": "expressts-cli",
  "version": "0.0.1",
  "description": "TypeScript Express Start Project generator",
  "main": "src/index.ts",
  "scripts": {
    "build": " npm run clean && npm run compiler && npm run cp:templates ",
    "compiler": "tsc -p .",
    "clean": "shx rm -rf bin",
    "cp:templates": "shx cp -R src/templates bin/"
  },
  "bin": {
    "exp-g": "./bin/index.js"
  },
  "keywords": [
    "cli",
    "create-project"
  ],
  "author": "tranvd2010",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.7.5",
    "@types/shelljs": "^0.8.5",
    "shx": "^0.3.2",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "commander": "^3.0.1",
    "inquirer": "^7.0.0"
  }
}
