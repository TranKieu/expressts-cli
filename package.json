{
  "name": "expressts-cli",
  "version": "1.1.0",
  "description": "TypeScript Express Start Project generator",
  "main": "src/index.ts",
  "scripts": {
    "build": " npm run clean && npm run compiler && npm run cp:templates ",
    "compiler": "tsc -p .",
    "clean": "shx rm -rf bin",
    "cp:templates": "shx cp -R src/templates bin/",
    "update": "npm update"
  },
  "bin": {
    "exp-g": "./bin/index.js"
  },
  "keywords": [
    "cli",
    "create-project"
  ],
  "author": "tranvd2010 <tranvd2010@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.19.11",
    "shx": "^0.3.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "inquirer": "^7.3.3",
    "package-json": "^6.5.0"
  }
}
